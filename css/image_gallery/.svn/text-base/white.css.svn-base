}
a{
	color: #27D;
}
h2 {
	color: #333;
}
a.play,a.pause {
	position:absolute;
	top:-53px;
}
div#page {
	background-color: #fff;
	border-color: #ddd;
}
div#footer {
	color: #777;
}
div.caption-container  {
	color:#111111;
	margin-top:12px;
}
div.image-title {
	font-weight: bold;
	font-size: 1.4em;
	background-color:#fff;
}
div.image-desc {
	line-height: 1.3em;
	padding-top: 12px;
}
div.download {
	margin-top: 8px;
}
div.photo-index {
	color: #777;
}
div.navigation a.prev {
	background-color:white;
	background-image: url(../../../../prebuilt/interface_textures/image_gallery/2_0/previous_group.png);
	height:80px;
	width:91px;
	top:-7px;
	*top:-5px;
	left:15px;
}
