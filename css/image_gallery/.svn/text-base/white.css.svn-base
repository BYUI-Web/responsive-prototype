div.navigation a.next {
	background-color:white;
	background-image:url("../../../../prebuilt/interface_textures/image_gallery/2_0/next_group.png");
	height:80px;
	position:absolute;
	right:10px;
	top:2px;
	*top:4px;
	width:20px;
}
div.loader {
	background-image: url(../../../../Images/common/image_gallery/loader.gif);
}
div.slideshow img {
	border-color: #ccc;
	margin-left: auto;
	margin-right:auto;
}
ul.thumbs li.selected a.thumb {
	background: #000;
}
div.pagination a:hover {
	background-color: #eee;
}
div.pagination span.current {
	background-color: #000;
	border-color: #000;
	color: #fff;
}
#contentWrapper {
	overflow:hidden;
}